{
    "compilerOptions": {
        "target": "es2015",
        "allowJS": true,
        "skipLibCheck": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noEmit": true,
        "jsx": "preserve",
        "isolatedModules": true,
    },
    "include": [
        "src"
    ]
}
